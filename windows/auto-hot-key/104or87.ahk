; cap lctrl replace
Capslock::LCtrl
LCtrl::Capslock

; for mac custom
;<!::<# ; lalt lwin replace
;<#::<!
LAlt & x::send {Ctrl down}{x down}{Ctrl up}{x up}
LAlt & c::send {Ctrl down}{c down}{Ctrl up}{c up}
LAlt & v::send {Ctrl down}{v down}{Ctrl up}{v up}
LAlt & z::send {Ctrl down}{z down}{Ctrl up}{z up}
LAlt & y::send {Ctrl down}{y down}{Ctrl up}{y up}
LAlt & s::send {Ctrl down}{s down}{Ctrl up}{s up}
LAlt & f::send {Ctrl down}{f down}{Ctrl up}{f up}

; for flico shortcuts
Space::Space
Space & E::send {Up}
Space & D::send {Down}
Space & S::send {Left}
Space & F::send {Right}
Space & J::send {Ins}
Space & M::send {Del}
Space & K::send {Home}
Space & ,::send {End}
Space & L::send {PgUp}
Space & .::send {PgDn}
Space & `;::send {BS}
LShift::LShift
